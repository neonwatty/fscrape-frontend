    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "ci": "npm run check-all && npm run test:all",
    "validate:export": "npm run build && node scripts/validate-export.js",
    "preview:static": "npm run build && npx serve out -p 3001"
